{
  "name": "kingame96",
	"displayName": "QPROPERTY_GENERATOR",
	"description": "Helping generate QPROPERTY and related function",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.85.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:kingame96.GenerateQPROPERTY"
	],
	"main": "./extension.js",
	"contributes": {
		"name": "QPROPERTY Generate Code",
		"commands": [{
            "command": "kingame96.GenerateQPROPERTY",
            "title": "QPROPERTY Generate Code"

		}],
		"keybindings": [
			{
				"command": "kingame96.GenerateQPROPERTY",
				"key": "alt+M"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "kingame96.GenerateQPROPERTY",
					"group": "QPROPERTY Generate Code",
					"when": "true"
				}
			]}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.85.1",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.2"
    }

}
